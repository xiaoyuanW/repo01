function doad(datastr,id){
//getid(id).innerHTML = datastr;
if($("#"+id) && $("#"+id).text()=="" && datastr!=""){
$("#"+id).html(datastr);
}
/*
setTimeout(function () {
        $("#"+id).html(datastr);
    }, 1000);
*/
}
